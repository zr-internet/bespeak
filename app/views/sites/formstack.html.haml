- content_for :head do
  :javascript
    Bespeak.CourseTypes = #{ render(template: 'course_types/index', formats: [:json], handlers: [:rabl]) };
    Bespeak.Offices = #{ render(template: 'offices/index', formats: [:json], handlers: [:rabl]) };
  :javascript
    setTimeout(function(){var a=document.createElement("script");
    var b=document.getElementsByTagName("script")[0];
    a.src=document.location.protocol+"//dnn506yrbagrg.cloudfront.net/pages/scripts/0013/9024.js?"+Math.floor(new Date().getTime()/3600000);
    a.async=true;a.type="text/javascript";b.parentNode.insertBefore(a,b)}, 1);

#form
  %section.intro.row-fluid
    %header
      .progress
        .bar.bar-success.active{:style => "width:34%;"}= @form.value_for("progress_step_1")
        .bar{:style => "width: 33%;"}= @form.value_for("progress_step_2")
        .bar{:style => "width: 33%;"}= @form.value_for("progress_step_3")
        
    = render partial: 'select_date'
    = render partial: 'select_course'
    = render partial: 'pay_cash'
    = render partial: 'pay_credit_card'
    = render partial: 'pay_coupon'